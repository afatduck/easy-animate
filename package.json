{
  "name": "easy-animate",
  "version": "1.0.1",
  "description": "A simple library to make using CSS animations easier. It can be used to define, start and stop animations.",
  "main": "dist/index.min.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "watch": "webpack --config demo/webpack.config.js",
    "serve": "cd demo && live-server",
    "start": "concurrently \"npm run watch\" \"npm run serve\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afatduck/easy-animate.git"
  },
  "keywords": [
    "CSS",
    "Animation",
    "Easy"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/afatduck/easy-animate/issues"
  },
  "homepage": "https://github.com/afatduck/easy-animate#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "bundle-declarations-webpack-plugin": "^3.1.0",
    "concurrently": "^7.2.2",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "live-server": "^1.2.2",
    "prettier": "^2.7.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "csstype": "^3.1.0"
  },
  "files": [
    "dist"
  ]
}
